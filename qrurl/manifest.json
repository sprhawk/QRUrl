{
        "name":"making QRCode of the url",
        "version":"1.0.1",
        "manifest_version":2,
        "description":"making QRCode of the url",
        "icons":{
            "16":"icon16.png",
            "32":"icon32.png",
            "128":"icon128.png",
            "256":"icon256.png"
        },
        "permissions":[
            "http://*/*",
            "https://*/*",
            "tabs"
        ],
        
        "background": {
            "scripts":["qrcodejs/qrcode.js", "background.js"]
        },
        "browser_action": {
            "default_icon":"icon128.png",
            "default_title":"QRCode URL",
            "default_popup": "popup.html"
        }
}
